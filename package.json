{
  "name": "qotd",
  "version": "0.2.1",
  "license": "MIT",
  "author": "James Dean \"DefiantBidet\"",
  "main": "./lib/cli.js",
  "description": "outputs a quote of the day in your shell.",
  "keywords": [
    "quote",
    "qotd",
    "cli"
  ],
  "scripts": {
    "lint": "eslint -c .eslintrc -f visualstudio ./lib/",
    "test": "NODE_ENV=test ava -v",
    "test:watch": "NODE_ENV=test ava -wv",
    "coverage": "NODE_ENV=test nyc --reporter=lcov yarn test && nyc report",
    "covReport": "nyc report"
  },
  "ava": {
    "files": [
      "*.spec.js",
      "**/*.spec.js"
    ]
  },
  "bin": {
    "qotd": "./lib/cli.js"
  },
  "files": [
    "./lib/index.js",
    "./lib/cli.js",
    "./lib/quotes.json"
  ],
  "devDependencies": {
    "ava": "3.8.2",
    "eslint": "7.1.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.20.2",
    "mock-require": "3.0.3",
    "nock": "12.0.3",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "sinon": "9.0.2"
  },
  "dependencies": {
    "chalk": "4.0.0",
    "cli-spinners": "2.3.0",
    "is-online": "8.4.0",
    "ora": "4.0.4",
    "yargs": "15.3.1"
  }
}
